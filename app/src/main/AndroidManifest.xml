        <activity
            android:name=".FollowListActivity"
            android:exported="false" />

        <activity
            android:name=".MyMapActivity"
            android:exported="false"
            android:launchMode="singleTop" />

        <activity
            android:name=".UserLogMapActivity"
            android:exported="false" />

        <activity
            android:name=".PlacePostListActivity"
            android:exported="false" />

        <activity
            android:name=".UserLogListActivity"
            android:exported="false" />

